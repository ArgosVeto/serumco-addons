<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="privacy_policy" model="ir.ui.view">
		<field name="name">Privacy Policy</field>
		<field name="type">qweb</field>
		<field name="key">website.privacy_policy</field>
		<field name="arch" type="xml">
			<t name="Privacy Policy" t-name="privacy_policy">
				<t t-call="website.layout">
					<div class="oe_structure oe_empty" id="wrap">
					</div>
				</t>
			</t>
		</field>
	</record>
	<record id="privacy_policy_menu" model="website.page">
		<field name="website_published">True</field>
		<field name="url">/privacy-policy</field>
		<field name="view_id" ref="privacy_policy" />
	</record>
</odoo>