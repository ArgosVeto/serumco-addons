					</strong>
				</t>
			</t>
			<strong class="text-muted d-none d-md-block small">
				<t t-esc="line.name_short_without_code()"/>
			</strong>
		</xpath>

		<xpath expr="//a[hasclass('btn','btn-primary')]" position="replace">
			<a role="button" class="btn btn-primary" href="/shop/cart">
               Voir mon panier
			</a>
		</xpath>
	</template>

	<!--hide attribute on product view-->
	<template id="sale_variants_inherit" inherit_id="sale.variants">
		<xpath expr="//t[@t-foreach='product.valid_product_template_attribute_line_ids']" position="replace"/>
	</template>

	<record id="product_category_form_view_inherit" model="ir.ui.view">
		<field name="name">Add website description</field>
		<field name="model">product.category</field>
		<field name="inherit_id" ref="product.product_category_form_view" />
		<field name="arch" type="xml">
			<data>
				<xpath expr="//group[@name='account_property']" position="after">
					<label for="web_visible" string="Visible sur le site "/>
					<field name="web_visible"/>
					<group string="Description pour le site">
						<field name="website_description"/>
					</group>
				</xpath>
			</data>
		</field>
	</record>
