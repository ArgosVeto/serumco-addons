<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>

		<record id="call_log_tree_view" model="ir.ui.view">
			<field name="name">call.log.tree.view</field>
			<field name="model">call.log</field>
			<field name="arch" type="xml">
				<tree string="Call Logs" create="0" delete="0" decoration-danger="status != 200">
					<field name="user_id"/>
					<field name="caller_number"/>
					<field name="date"/>
					<field name="partner_id"/>
					<field name="callee_number"/>
					<field name="type"/>
					<field name="status"/>
				</tree>
			</field>
		</record>

		<record id="call_log_form_view" model="ir.ui.view">
			<field name="name">call.log.form.view</field>
			<field name="model">call.log</field>
			<field name="arch" type="xml">
				<form string="Call Log" create="0" edit="0" delete="0">
					<sheet>
						<group>
							<group>
								<field name="type"/>
								<field name="user_id"/>
								<field name="caller_number"/>
								<field name="partner_id"/>
								<field name="callee_number"/>
							</group>
							<group>
								<field name="date"/>
								<field name="status"/>
								<field name="message"/>
							</group>
						</group>
						<group name="url_keyyo" string="URL &amp; Parameters">
							<field name="url" nolabel="1"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record id="call_log_action" model="ir.actions.act_window">
			<field name="name">Call Logs</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">call.log</field>
			<field name="view_mode">tree,form</field>
			<field name="help" type="html">
				<p class="oe_view_nocontent_create">
					<!-- Add Text Here -->
				</p>
				<p>
					<!-- More details about what a user can do with this object will be OK -->
				</p>
			</field>
		</record>
		
		<!-- This Menu Item Must have a parent -->
		<menuitem id="keyyo_cti_categ" name="Keyyo CTI" parent="contacts.menu_contacts" sequence="10"/>
		<!-- This Menu Item must have a parent and an action -->
		<menuitem id="call_log_keyyo_categ" name="Call Logs" parent="keyyo_cti_categ" action="call_log_action" sequence="10"/>

	</data>
</odoo>