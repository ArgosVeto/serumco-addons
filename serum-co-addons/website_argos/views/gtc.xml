<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="gtc" model="ir.ui.view">
		<field name="name">GTC</field>
		<field name="type">qweb</field>
		<field name="key">website.gtc</field>
		<field name="arch" type="xml">
			<t name="GTC" t-name="gtc">
				<t t-call="website.layout">
					<div class="oe_structure oe_empty" id="wrap">
					</div>
				</t>
			</t>
		</field>
	</record>
	<record id="gtc_menu" model="website.page">
		<field name="website_published">True</field>
		<field name="url">/gtc</field>
		<field name="view_id" ref="gtc" />
	</record>
</odoo>