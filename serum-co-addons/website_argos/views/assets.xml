	<template id="website_argos_asset_frontend" inherit_id="website.assets_frontend" name="Bizople Theme Assets">
        <xpath expr="//link[last()]" position="after">
            <link href="/website_argos/static/src/css/owl.carousel.css" rel="stylesheet" type="text/css"/>
            <link href="/website_argos/static/src/css/ion.rangeSlider.css" rel="stylesheet" type="text/css"/>
            <link href="/website_argos/static/src/css/ion.rangeSlider.skinHTML5.css" rel="stylesheet" type="text/css"/>
            <link href="/website_argos/static/src/scss/pro_slider.scss" rel="stylesheet" type="text/scss"/>

        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script  src="/website_argos/static/src/js/owl.carousel.min.js" type="text/javascript"></script>
            <script src="/website_argos/static/src/js/bizcommon_fronted.js" type="text/javascript"/>
            <script type="text/javascript" src="/website_argos/static/src/js/ion.rangeSlider.min.js" />
            <script type="text/javascript" src="/website_argos/static/src/js/common.js" />
			<script src="/website_argos/static/src/js/biz_quickview.js" />
			<script src="/website_argos/static/src/js/product_variant.js" />
			<script src="/website_argos/static/src/js/cart_address.js"/>
			<script src="/website_argos/static/src/js/chatter.js"/>
			<script src="/website_argos/static/src/js/portal_chatter.js"/>
			<script src="/website_argos/static/src/js/resume_upload.js"/>

        </xpath>


