<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
    	<record id="view_product_template_sku_field" model="ir.ui.view">
            <field name="name">product.template.form</field>
            <field name="model">product.template</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="product.product_template_only_form_view" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='categ_id']" position="after">
                    <field name="tag_ids" widget="many2many_tags"/>
                </xpath>
            </field>
        </record>
        
        
       <record id="product_attribute_view_form_inh" model="ir.ui.view">
        <field name="name">product.attribute.form.inherit.argos</field>
        <field name="model">product.attribute</field>
        <field name="inherit_id" ref="sale.product_attribute_view_form"/>
        <field name="priority" eval="8"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='value_ids']/tree/field[@name='is_custom']" position="after">
                <field name="image"/>
            </xpath>
        </field>
    </record>

    </data>
</odoo>