                                Bienvenue <span><t t-esc="request.env.user.partner_id.name"/></span>
                            </h1>
                            <p>
                                Retrouvez sur cette page lorem ipsum dolor sit amet 
                            </p>
                            <p class="mb48">
                                Nous Contacter
                            </p>
                        </div>
                        <div class="col-md-12 main-content">
                            <div class="row">
                                <div class="col-md-4 o-1">
                                    <div class="card p-3">
                                        <div class="row title-row">
                                            <div class="col-md-6 col-6 text-left">
                                                <strong>MES COMMANDES</strong>
                                            </div>
                                            <div class="col-md-6 col-6 text-right">
                                                <a href="/my/orders">Voir tout</a>
                                            </div>
                                        </div>
                                        <div>
                                            <t t-if="order_ids">
                                                <t t-set="order_number" t-value="len(order_ids)"/>
                                                 <t t-set="order_counter" t-value="0"/>
                                                <t t-foreach="order_ids" t-as="order">
                                                    <t t-if="order.order_line">
                                                    <t t-set="order_counter" t-value="order_counter+1"/>
                                                    <t t-foreach="order.order_line[0]" t-as="line">
                                                        <t t-if="order_counter > 1">
                                                            <t t-set="domainClass" t-value="'row mx-0 px-12'"/>
                                                        </t>
                                                        <t t-else="">
