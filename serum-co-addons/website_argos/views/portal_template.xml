                                                                    <form id="update_address" action="/add-product-cart" method="post" enctype="multipart">
                                                                    <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()" />

                                                                    <div class="row mt24 mes-product">
                                                                        <div class="col-md-6 col-6">
                                                                            <span t-field="fav_pro.product_id.image_1920"
