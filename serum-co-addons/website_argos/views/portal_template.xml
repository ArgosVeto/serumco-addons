<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <template id="my_appointment_template" name="my appointment">
        <div class="mb10" t-if="record">
            <div class="row mt24 mon-prochain">
                <div class="col-md-12">
                    <div class="date-time">
                        <t t-set="week_day" t-value="record.get_day_name(record.start_datetime)"/>
                        <span t-esc="week_day and week_day.title()"/>
                        <span t-field="record.start_datetime" t-options='{"format": "d MMMM yyyy"}'/>
                        <span>-</span>
                        <span t-field="record.start_datetime" t-options='{"format": "HH:mm"}'/>
                    </div>
                </div>
            </div>
            <div class="row mt24 mon-prochain">
                <div class="col-md-4 col-4">
                    <img class="img img-fluid" t-att-src="website.image_url(record.operating_unit_id, 'clinic_banner')" />
                </div>
                <div class="col-md-8 col-8 main-info">
                    <strong t-esc="record.operating_unit_id.name"/>
                    <div class="mb24" t-if="record.employee_id">
                       <span>Dr. <t t-esc="record.employee_id.name"/></span>
                    </div>
                </div>
            </div>
        </div>
         <t t-if="record and add_to_my_calendar">
             <a role="button" t-att-href="'/planning/%s/ics' % record.id" class="btn btn-primary btn-lg mt-auto d-block d-sm-inline-block link-btn">Ajouter a mon calendrier</a>
         </t>
    </template>

   <!--  reqw -->
    <template id="portal_template" name="Portal Template">
        <t t-call="website.layout">
            <div id="wrap"  class="">
                <div class="row m-0 mx-md-3 my_content">
                    <div class="col-lg-12 mb-0 mb-md-5 p-0 p-md-3">
                        <div class="top-links p-0">
                            <div class="container">
                                <ul class="nav navbar-nav flex-row">
                                    <li class="nav-item mr32">
                                        <a class="nav-link active" href="/my-content">Vue d'ensemble</a>
                                    </li>
                                    <li class="nav-item mr32">
                                        <a class="nav-link" href="/my/orders">Commandes</a>
                                    </li>
                                    <t t-if="next_appointment_id or pervious_appointment_id">
                                        <li class="nav-item mr32">
                                            <a class="nav-link" href="/my-appointment">Mes RDV</a>
                                        </li>
                                    </t>
                                    <li class="nav-item mr32">
                                        <a class="nav-link" href="/my/account">Mes adresses</a>
                                    </li>
                                    <li class="nav-item mr32">
                                        <a class="nav-link" href="/my-clinic">Ma clinique</a>
                                    </li>
                                    <li class="nav-item mr32">
                                        <a class="nav-link" href="/shop/wishlist">Mes produits favoris</a>
                                    </li>
                                    <li class="nav-item mr32">
                                        <a class="nav-link" href="/my/home">Mes infos personnelles</a>
                                    </li>
                                    <!-- <li class="nav-item m-0">
                                        <a class="nav-link" href="/my/payment_method">Mes cartes bancaires</a>
                                    </li> -->
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
