                                                    <div class="col-md-6 col-6 text-left">
                                                        <strong>MES PRODUITS FAVORIS</strong>
                                                    </div>
                                                    <div class="col-md-6 col-6 text-right">
                                                        <a href="/shop/wishlist">Voir tout</a>
                                                    </div>
                                                </div>
                                                <div class="mb32">
                                                    <t t-if="product_wishlist_ids">
                                                        <div id="fav-product-carousel" class="mes-product carousel bs-slider slide  control-round indicators-line mt24" data-ride="carousel" data-pause="hover" data-interval="7000">
                                                            <div class="carousel-inner" role="listbox">
                                                                <t t-set="count" t-value="0"/>
                                                                <t t-foreach="product_wishlist_ids" t-as="fav_pro">
                                                                    <t t-if="count == 0">
                                                                        <div class="carousel-item active" >
                                                                            <form id="update_address" action="/add-product-cart" method="post" enctype="multipart">
                                                                            <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()" />

                                                                            <div class="row mt24 mes-product">
                                                                                <div class="col-md-6 col-6">
                                                                                    <span t-field="fav_pro.product_id.image_1920"
                                                                                        t-options="{'widget': 'image', 'preview_image': 'image_1024' if product_image_big else 'image_256'}"
                                                                                        class="wishlist-product-img d-flex h-100 justify-content-center align-items-center"/>
                                                                                </div>
                                                                                <div class="col-md-6 col-6">
                                                                                    <p t-field="fav_pro.product_id.public_categ_ids"/>
                                                                                    <strong><t t-esc="fav_pro.product_id.name"/></strong>
                                                                                </div>
                                                                            </div>
                                                                            <input name="product_id" t-att-value="fav_pro.product_id.id" type="hidden"/>
                                                                            <button type="submit" id="form_submit"
                                                                            class="btn btn-primary w-100 btn-lg d-block mt-5 o_wish_add"><img src="/website_argos/static/src/img/white-cart.png" class="img img-fluid mr8"/>
                                                                            Ajouter
                                                                        </button>
                                                                        </form>
                                                                        </div>
                                                                    </t>
                                                                    <t t-else="">
                                                                        <div class="carousel-item" >
                                                                            <form id="update_address" action="/add-product-cart" method="post" enctype="multipart">
                                                                            <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()" />

                                                                            <div class="row mt24 mes-product">
                                                                                <div class="col-md-6 col-6">
                                                                                    <span t-field="fav_pro.product_id.image_1920"
                                                                                        t-options="{'widget': 'image', 'preview_image': 'image_1024' if product_image_big else 'image_256'}"
                                                                                        class="wishlist-product-img d-flex h-100 justify-content-center align-items-center"/>
                                                                                </div>
                                                                                <div class="col-md-6 col-6">
                                                                                    <p t-field="fav_pro.product_id.public_categ_ids"/>
                                                                                    <strong><t t-esc="fav_pro.product_id.name"/></strong>
                                                                                </div>
                                                                            </div>
                                                                            <input name="product_id" t-att-value="fav_pro.product_id.id" type="hidden"/>
                                                                            <button type="submit" id="form_submit"
                                                                            class="btn btn-primary w-100 btn-lg d-block mt-5 o_wish_add"><img src="/website_argos/static/src/img/white-cart.png" class="img img-fluid mr8"/>
                                                                            Ajouter
                                                                        </button>
                                                                        </form>
                                                                        </div>
                                                                    </t>
                                                                    <t t-set="count" t-value="count + 1"/>
                                                                </t>
                                                            </div>
                                                            <div class="slider-control">
                                                                <a class="left carousel-control-prev font-14" data-target="#fav-product-carousel" role="button" data-slide="prev">
                                                                    <img src="/website_argos/static/src/img/left.svg"/>
                                                                    <b class="sr-only font-normal o_default_snippet_text">Previous</b>
                                                                </a>
                                                                <a class="right carousel-control-next font-14" data-target="#fav-product-carousel" role="button" data-slide="next">
                                                                    <img src="/website_argos/static/src/img/right.svg"/>
                                                                    <b class="sr-only font-normal o_default_snippet_text">Next</b>
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </t>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </t>
    </template>
    

    <template id="clinic_shortlisted_template" name="Clinic Shortlisted">
        <t t-call="website.layout">
            <div id="wrap"  class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="row">
                            <t t-foreach="clinic_shortlisted_ids" t-as="clinic">
                                <div class="col-lg-4 s_col_no_bgcolor pt16 pb16 clinic-grid">
                                    <div class="card bg-white">
                                        <img class="card-img-top" t-att-src="website.image_url(clinic.favorite_clinic_id, 'clinic_banner')"/>
                                        <div class="card-body">
                                            <h3 class="card-title o_default_snippet_text">
                                                <a itemprop="display_name"  t-attf-href="/clinic-detail/clinic-pratice/#{clinic.favorite_clinic_id.id}" t-att-content="clinic.favorite_clinic_id.display_name" t-field="clinic.favorite_clinic_id.display_name"/>
                                            </h3>

                                            <p class="Clinique-actuellemen">
                                                <i class="fa fa-circle"></i><span>Clinique actuellement ouverte</span>
                                            </p>
                                            
                                            <div class="clinic-link d-flex">
                                                <div class="image-l">
                                                    <img src="/website_argos/static/src/img/location.svg"/>
                                                </div>
                                                <div class="link">
                                                    <span class="Avenue-Pasteur">
                                                        <span class="street" t-field="clinic.favorite_clinic_id.street"/><span>, </span><span class="zip" t-field="clinic.favorite_clinic_id.zip"/><span>, </span><span class="city" t-field="clinic.favorite_clinic_id.city"/>
                                                    </span>
                                                </div>
                                            </div>
                                                
                                            <div class="">
                                                 <a role="button" target="" t-attf-href="tel:{{clinic.favorite_clinic_id.phone}}" class="btn btn-outline-primary mb16 mt16 d-none d-xl-inline-block mr-2">
                                                    <img src="/website_argos/static/src/img/phone.svg" class="img img-fluid mr-2"/><span class="">Appeler</span>
                                                </a>
                                                 <a role="button" target="_blank" href="#" class="btn btn-primary mb16 mt16 d-none d-xl-inline-block">
                                                    <span class="">Prendre RDV</span>
                                                </a>
                                            </div>                                          
                                        </div>
                                    </div>
                                </div>
                            </t>
                        </div>
                    </div>
                </div>
            </div>
        </t>
    </template>


