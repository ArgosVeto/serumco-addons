<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="legal_notice" model="ir.ui.view">
		<field name="name">Legal Notice</field>
		<field name="type">qweb</field>
		<field name="key">website.legal_notice</field>
		<field name="arch" type="xml">
			<t name="Legal Notice" t-name="legal_notice">
				<t t-call="website.layout">
					<div class="oe_structure oe_empty" id="wrap">
					</div>
				</t>
			</t>
		</field>
	</record>
	<record id="legal_notice_menu" model="website.page">
		<field name="website_published">True</field>
		<field name="url">/legal-notice</field>
		<field name="view_id" ref="legal_notice" />
	</record>
</odoo>