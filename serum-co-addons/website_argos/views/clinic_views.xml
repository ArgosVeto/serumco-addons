<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="clinic_image_form_view" model="ir.ui.view">
        <field name="name">clinic.image.form.view</field>
        <field name="model">clinic.image</field>
        <field name="arch" type="xml">
            <form string="Clinic Image">
                <sheet>
                    <group>
                        <group>
                            <field name="name" invisible="1"/>
                            <field name="image_1920" filename="name"/>

                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

	<record id="clinic_favorite_form_view" model="ir.ui.view">
		<field name="name">clinic.favorite.form.view</field>
		<field name="model">clinic.favorite</field>
		<field name="arch" type="xml">
			<form string="Clinic">
				<sheet>
					<group>
                        <group>
							<field name="favorite_clinic_id"/>
						</group>
					</group>
				</sheet>
			</form>
		</field>
	</record>


