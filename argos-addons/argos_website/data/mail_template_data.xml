<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
  <data>
    <record id="rating_mail_template_data" model="mail.template">
      <field name="name">Rating submitted</field>
      <field name="model_id" ref="rating.model_rating_rating"/>
      <field name="subject">Rating submitted on your website</field>
      <field name="lang">${object.message_id.author_id.lang}</field>
      <field name="partner_to">${object.get_mail_recipients()}</field>
      <field name="auto_delete" eval="False"/>
      <field name="body_html">
        <![CDATA[
          <span>A Ratting were submitted on your website for <a href=${object.get_url()}>${object.res_name}</a> </span>
      ]]>
      </field>
    </record>
  </data>
</odoo>