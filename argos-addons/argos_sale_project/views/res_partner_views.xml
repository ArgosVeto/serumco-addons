<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record id="res_partner_view_form_inherit" model="ir.ui.view">
      <field name="name">res.patient.view.form.inherit</field>
      <field name="model">res.partner</field>
      <field name="inherit_id" ref="argos_contact.res_partner_view_form"/>
      <field name="arch" type="xml">
        <page name="analysis" position="inside">
          <field name="result_line_ids" readonly="1">
            <tree default_order="id asc">
              <field name="task_id" invisible="1"/>
              <field name="name"/>
              <field name="result_date"/>
              <field name="parameter_id"/>
              <field name="value"/>
              <field name="min"/>
              <field name="max"/>
              <field name="unit"/>
              <field name="comments"/>
            </tree>
          </field>
        </page>
        <page name="imagery" position="inside">
          <field name="imagery_task_ids" readonly="1">
            <tree default_order="id asc">
              <field name="name"/>
              <field name="result_date"/>
              <field name="type"/>
              <field name="equipment_id"/>
              <field name="imagery" widget="image" options="{'preview_image': 'image_128', 'size': [80, 80]}"
                     string="Result"/>
              <field name="comments"/>
            </tree>
          </field>
        </page>
      </field>
    </record>
  </data>
</odoo>